{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "clusterName": {
      "value": "GENERATE-CLUSTER-NAME"
    },
    "adminUsername": {
      "value": "ADMIN-USERNAME"
    },
    "adminPassword": {
      "value": "ADMIN-PASSWORD"
    },
